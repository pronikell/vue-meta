{"code":"import { inject, getCurrentInstance, isProxy, watch } from 'vue';\r\nimport { metaActiveKey } from './symbols';\r\nimport { applyDifference } from './utils/diff';\r\nexport function getCurrentManager(vm) {\r\n    if (!vm) {\r\n        vm = getCurrentInstance() || undefined;\r\n    }\r\n    if (!vm) {\r\n        return undefined;\r\n    }\r\n    return vm.appContext.config.globalProperties.$metaManager;\r\n}\r\nexport function useMeta(source, manager) {\r\n    const vm = getCurrentInstance() || undefined;\r\n    if (!manager && vm) {\r\n        manager = getCurrentManager(vm);\r\n    }\r\n    if (!manager) {\r\n        throw new Error('No manager or current instance');\r\n    }\r\n    if (isProxy(source)) {\r\n        watch(source, (newSource, oldSource) => {\r\n            applyDifference(metaProxy.meta, newSource, oldSource);\r\n        });\r\n        source = source.value;\r\n    }\r\n    const metaProxy = manager.addMeta(source, vm);\r\n    return metaProxy;\r\n}\r\nexport function useActiveMeta() {\r\n    return inject(metaActiveKey);\r\n}\r\n//# sourceMappingURL=useApi.js.map","references":["/tmp/b/vue-meta/node_modules/vue/dist/vue.d.ts","/tmp/b/vue-meta/src/symbols.ts","/tmp/b/vue-meta/src/utils/diff.ts","/tmp/b/vue-meta/src/manager.ts","/tmp/b/vue-meta/src/types/index.ts"],"map":"{\"version\":3,\"file\":\"useApi.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../src/useApi.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,MAAM,EAAE,kBAAkB,EAA6B,OAAO,EAAE,KAAK,EAAE,MAAM,KAAK,CAAA;AAC3F,OAAO,EAAE,aAAa,EAAE,MAAM,WAAW,CAAA;AACzC,OAAO,EAAE,eAAe,EAAE,MAAM,cAAc,CAAA;AAI9C,MAAM,UAAU,iBAAiB,CAAE,EAA8B;IAC/D,IAAI,CAAC,EAAE,EAAE;QACP,EAAE,GAAG,kBAAkB,EAAE,IAAI,SAAS,CAAA;KACvC;IAED,IAAI,CAAC,EAAE,EAAE;QACP,OAAO,SAAS,CAAA;KACjB;IAED,OAAO,EAAE,CAAC,UAAU,CAAC,MAAM,CAAC,gBAAgB,CAAC,YAAY,CAAA;AAC3D,CAAC;AAED,MAAM,UAAU,OAAO,CAAE,MAAkB,EAAE,OAAqB;IAChE,MAAM,EAAE,GAAG,kBAAkB,EAAE,IAAI,SAAS,CAAA;IAE5C,IAAI,CAAC,OAAO,IAAI,EAAE,EAAE;QAClB,OAAO,GAAG,iBAAiB,CAAC,EAAE,CAAC,C